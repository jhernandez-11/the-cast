<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta http-equiv="X-UA-Compatible" content="ie=edge">

        <link href="https://fonts.googleapis.com/css2?family=Orbitron:wght@400;700&display=swap" rel="stylesheet">        
        <link rel="stylesheet" href="/css/style.css">
        <link rel="stylesheet" href="/css/jquery-ui.min.css">
        <script src="/js/anime.min.js"></script>
        <script src="/js/jquery.min.js"></script>
        <script src="/js/jquery-ui.min.js"></script>
        <link rel="shortcut icon" type="image/png" href="/img/favicon.png">

        <title>The Cast</title>
    </head>
    <body>
        <!--/////////////// POPUP ///////////////-->
        <div class="popup">
            <h3 class="popup__text">Currenly Unavailable!</h3>
            <div class="popup__text-p">(Click anywhere to close)</div>
        </div>

        <!--/////////////// HEADER ///////////////-->
        {{>headerv2}}

        <!--/////////////// SECTION ///////////////-->
        <section class="section">
            <div class="section__mainv2">
                <div class="section__info-container">
                    <h3 class="section__text">Frontend - </h3>
                    <h3 class="section__info">ANIME.JS</h3>
                    <h3 class="section__info">JQUERY</h3>
                    <h3 class="section__info">JQUERY UI</h3>
                    <h3 class="section__info">SASS</h3>
                    <h3 class="section__info">GOOGLE FONTS</h3>
                    <h3 class="section__info">NASA</h3>
                </div>

                <div class="section__info-container">
                    <h3 class="section__text">Backend - </h3>
                    <h3 class="section__info">Express</h3>
                    <h3 class="section__info">HBS</h3>
                    <h3 class="section__info">REQUEST</h3>
                    <h3 class="section__info">MAPBOX</h3>
                    <h3 class="section__info">WEATHER STACK</h3>
                    <h3 class="section__info">HEROKU</h3>
                </div>
            </div>
        </section>

        <!--/////////////// FOOTER ///////////////-->
        {{>footer}}

        <!--/////////////// SCRIPT ///////////////-->
        <script src="/js/app.js"></script>
        <script src="/js/index.js" async defer></script>
    </body>
</html>